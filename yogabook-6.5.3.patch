-	if (ret) {
-		dev_err(&rdev->dev, "error reading pmic, %x\n", ret);
-		return ret;
-	}
-
-	reg_val &= ~pmic_info->vctl_mask;
-	reg_val |= pmic_info->reg_dsbl_mask;
