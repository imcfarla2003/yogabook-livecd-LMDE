-	unsigned int reg_val;
-	int ret = 0;
-
-	ret = regmap_read(regmap, pmic_info->vctl_reg, &reg_val);
-	if (ret) {
-		dev_err(&rdev->dev, "error reading pmic, %x\n", ret);
-		return ret;
-	}
-
-	reg_val &= pmic_info->vctl_mask;
-
-	return reg_val & pmic_info->reg_enbl_mask;
