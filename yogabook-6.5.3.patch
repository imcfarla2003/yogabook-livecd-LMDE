-
-	dev_dbg(card->dev, "HP event: %s\n",
-		SND_SOC_DAPM_EVENT_ON(event) ? "ON" : "OFF");
-
-	if (SND_SOC_DAPM_EVENT_ON(event)) {
-		msleep(20);
-		gpiod_set_value_cansleep(ctx->gpio_hp_en, 1);
